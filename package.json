{
    "name": "manage_landing_page",
    "version": "1.0.0",
    "description": "![Design preview for the Manage landing page coding challenge](./design/desktop-preview.jpg)",
    "main": "index.js",
    "scripts": {
        "watch": "sass --watch src/scss/style.scss css/style.css",
        "start": "live-server",
        "compile": "sass src/scss/style.scss src/style.comp.css",
        "prefix": "postcss src/style.comp.css -u autoprefixer -o src/style.prefix.css",
        "compress": "sass src/style.prefix.css css/style.css --style compressed",
        "build": "npm-run-all compile prefix compress"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.2.6",
        "live-server": "^1.2.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1",
        "sass": "^1.35.2"
    }
}